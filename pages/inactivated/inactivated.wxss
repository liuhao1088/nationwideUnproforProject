page {
  background-color: #f2f2f2;
}

image {
  height: 100%;
  width: 100%;
  vertical-align: middle;
}

input {
  width: 100%;
  height: 100%;
  line-height: 84rpx;
  color: black;
}

button::after {
  border: 0;
}

.inactivatedContainer {
  padding-bottom: 180rpx;
}

.header {
  width: 90%;
  height: 340rpx;
  border: 0rpx solid red;
  border-radius: 30rpx;
  background-color: rgb(193, 193, 193);
  margin: 20rpx 0;
}

.user-item {
  height: 80%;
  width: 90%;
  border: 0rpx solid red;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;

}

.user-wrap {
  height: 68rpx;
  line-height: 68rpx;
  width: 100%;
  border: 0rpx solid black;
  display: flex;
}

.user-tagItem {
  height: 100%;
  color: rgba(255, 255, 255, 0.5);
  font-size: 28rpx;
}

.user-butItem {
  flex: 1;
  border: 0rpx solid red;
}

.user-but {
  width: 200rpx;
  height: 100%;
  float: right;
}

.user-but button {
  width: 100%;
  height: 100%;
  line-height: 68rpx;
  background-color: rgb(255, 255, 255);
  border: 2rpx solid rgb(23, 100, 152);
  color: rgb(23, 100, 152);
  font-size: 24rpx;
  padding: 0;
}



.img-item {
  height: 100%;
  width: 68rpx;
  background-color: red;
  border-radius: 50%;
  font-size: 28rpx;
}

.name-item {
  flex: 1;
  border: 0rpx solid red;
  padding-left: 20rpx;
}

/* 表单 */

.menu-item {
  width: 100%;
  height: 84rpx;
  line-height: 84rpx;
  border: 0rpx solid red;
  background-color: #fff;

}

.menu-wrap {
  width: 90%;
  height: 100%;
  margin: auto;
  display: flex;
  justify-content: space-between;
  border: 0rpx solid red;
  font-size: 32rpx;
}

.menu-leftItem {
  height: 100%;
  width: 200rpx;
  border: 0rpx solid red;
  display: flex;
}

.menu-rightItem {
  height: 100%;
  border: 0rpx solid red;
  display: flex;
  color: rgb(120, 120, 120);

}

.title-item {
  width: 93%;
  height: 84rpx;
  line-height: 84rpx;
  text-align: left;
  color: rgba(193, 193, 193);
  font-size: 32rpx;
}

.menu-msgItem {
  flex: 1;
  border: 0rpx solid red;
  color: rgb(136, 136, 136);
  font-size: 28rpx;
  padding-left: 50rpx;
}

.menu-imgItem {
  height: 76rpx;
  width: 76rpx;
  margin-top: 4rpx;
  border: 2rpx solid red;
}

.but-item {
  width: 100%;
  height: 120rpx;
  border-top: 2rpx solid #e8e8e8;
  background-color: #fff;
  position: fixed;
  bottom: 0;
  z-index: 1;
}

.but-item button {
  height: 80rpx;
  border-radius: 10rpx;
  text-align: center;
  background-color: rgba(23, 100, 152, 100);
  color: rgba(255, 255, 255, 100);
  font-size: 28rpx;
  padding: 0;
}

.yes-item {
  height: 100%;
  line-height: 84rpx;
  display: flex;
}

.yes-msg {
  margin-left: 20rpx;
}

.solids {
  height: 100%;
  width: 84rpx;

  position: relative;
}

.grid.grid-square>view {
  margin: 0;
}

.grid.grid-square .cu-tag {
  background-color: transparent;
}

.bg-red {
  height: 30rpx;
  width: 30rpx;
  line-height: 30rpx;
  color: red;
  font-weight: bold;
}


radio-group .wx-radio-input.wx-radio-input-checked {
  border: 1rpx solid rgb(23, 100, 152) !important;
  background: rgb(23, 100, 152) !important;
}

radio {
  transform: scale(0.8);
}

radio::before {
  color: #fff;
}

/* 延保套餐 */
.padding-xl {
  padding: 0rpx 60rpx;
  padding-bottom: 40rpx;
}

.cu-dialog {
  width: 100%;
  height: 1000rpx;
  border: 0rpx solid;
  /* background-color: unset; */
}